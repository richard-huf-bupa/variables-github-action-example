{
  "name": "figma-variables-gh-actions",
  "scripts": {
    "start": "remix-serve ./build/server/index.js",
    "build": "node ./src/build.mjs",
    "build:app": "remix vite:build",
    "dev": "remix vite:dev",
    "tokens:import:colour": "npx tsx ./src/syncFigmaToTokens.ts -- --output ./src/tokens/colour",
    "tokens:import:palette": "npx tsx ./src/syncFigmaToTokens.ts --isPalette=true  -- --output ./src/tokens/palette",
    "tokens:import": "npx tokens:import:palette && npx tokens:import:colour",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@mdx-js/react": "^3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@mui/base": "^5.0.0-beta.8",
    "@mui/material": "5.14.2",
    "@mui/system": "^6.4.8",
    "@mui/utils": "^6.4.8",
    "@remix-run/node": "^2.16.1",
    "@remix-run/react": "^2.16.0",
    "@remix-run/serve": "^2.16.0",
    "axios": "^1.8.1",
    "dotenv": "^16.4.7",
    "husky": "^9.1.7",
    "isbot": "^4.1.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^2.16.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@remix-run/dev": "^2.16.0",
    "@types/node": "^22.13.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "style-dictionary": "^4.3.3",
    "tsx": "^4.19.3",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "yargs": "^17.7.2"
  },
  "engines": {
    "node": ">=22.12.0"
  }
}